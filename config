#!/usr/bin/env bash
export AWSS3_IMAGE=${AWSS3_IMAGE:="convox/aws-s3"}
export AWSS3_IMAGE_VERSION=${AWSS3_IMAGE_VERSION:="latest"}
export AWSS3_ROOT=${AWSS3_ROOT:="/var/lib/dokku/services/awss3"}

export PLUGIN_COMMAND_PREFIX="aws-s3"
export PLUGIN_DATA_ROOT=$AWSS3_ROOT
export PLUGIN_DATASTORE_PORTS=(9324)
export PLUGIN_DEFAULT_ALIAS="AWSS3"
export PLUGIN_ALT_ALIAS="DOKKU_AWSS3"
export PLUGIN_IMAGE=$AWSS3_IMAGE
export PLUGIN_IMAGE_VERSION=$AWSS3_IMAGE_VERSION
export PLUGIN_SCHEME="http"
export PLUGIN_SERVICE="aws-s3"
export DOKKU_NOT_IMPLEMENTED_EXIT="exit 0";
